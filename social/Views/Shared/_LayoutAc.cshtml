    var i = 2;
    $(".menu-bar").click(function () {
        if (i % 2 == 0) {
            $(".div-menu-xs").css({ 'display': 'block' });
            i++;
        }
        else {
            $(".div-menu-xs").css({ 'display': 'none' });
            i++;
        }
    })
    $(".main-ac").click(function () {
        $(".div-menu-xs").css({ 'display': 'none' });

    })
</script>